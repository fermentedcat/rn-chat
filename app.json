{
  "expo": {
    "name": "snick-snack",
    "slug": "snick-snack",
    "owner": "fermentedcat",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/snick-snack.png",
    "splash": {
      "image": "./app/assets/snick-snack.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/snick-snack.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/snick-snack.png"
    },
    "description": "A simple chat application for Android and iOS.",
    "githubUrl": "https://github.com/fermentedcat/rn-chat"
  }
}
